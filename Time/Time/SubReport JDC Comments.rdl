        </Field>
        <Field Name="JOB_DAY_CREW_ID">
          <DataField>JOB_DAY_CREW_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
