  <DataSets>
    <DataSet Name="Master">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@JDCID">
            <Value>=Parameters!JDCID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        UPRO_JOB_DAY_CREW.JOB_DAY_ID, UPRO_PERSON.FULL_NAME AS Foreman, COALESCE (CAST(UPRO_JOB_DAY_CREW_TASK.TASK_COMMENT AS VARCHAR(1000)), 'No Comments') AS JDCComments, 
                         UPRO_PERSON.LAST_NAME, UPRO_JOB_DAY_CREW.JOB_DAY_CREW_ID
FROM            UPRO_JOB_DAY_CREW_TASK RIGHT OUTER JOIN
                         UPRO_JOB_DAY_CREW ON UPRO_JOB_DAY_CREW_TASK.JOB_DAY_CREW_ID = UPRO_JOB_DAY_CREW.JOB_DAY_CREW_ID LEFT OUTER JOIN
                         UPRO_PERSON ON UPRO_JOB_DAY_CREW.FOREMAN_ID = UPRO_PERSON.PERSON_ID
WHERE        (UPRO_JOB_DAY_CREW.JOB_DAY_CREW_ID = @JDCID)
ORDER BY UPRO_PERSON.LAST_NAME</CommandText>
      </Query>
      <Fields>
        <Field Name="JOB_DAY_ID">
          <DataField>JOB_DAY_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Foreman">
