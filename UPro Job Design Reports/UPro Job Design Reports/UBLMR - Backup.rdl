    <DataSet Name="PhaseList">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@T">
            <Value>=Parameters!T.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        'Part ' + UPRO_PHASE_CODE.CODE_NUM + ' - ' + UPRO_PHASE_CODE.DESCR AS Phase, UPRO_PHASE_CODE.SORT_ORDER AS PhaseSort, UPRO_PHASE_CODE.PHASE_CODE_ID, 
                         ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME
FROM            ARPT_REPORT_REQUEST LEFT OUTER JOIN
                         UPRO_JOB_LOCATION RIGHT OUTER JOIN
                         UPRO_JOB_DESIGN ON UPRO_JOB_LOCATION.JOB_DESIGN_ID = UPRO_JOB_DESIGN.JOB_DESIGN_ID LEFT OUTER JOIN
                         UPRO_JOB_LOCATION_DETAIL LEFT OUTER JOIN
                         UPRO_PHASE_CODE RIGHT OUTER JOIN
                         UPRO_SPECIFICATION_ACTION ON UPRO_PHASE_CODE.PHASE_CODE_ID = UPRO_SPECIFICATION_ACTION.PHASE_CODE_ID ON 
                         UPRO_JOB_LOCATION_DETAIL.SPECIFICATION_ACTION_ID = UPRO_SPECIFICATION_ACTION.SPECIFICATION_ACTION_ID ON 
                         UPRO_JOB_LOCATION.JOB_LOCATION_ID = UPRO_JOB_LOCATION_DETAIL.JOB_LOCATION_ID ON ARPT_REPORT_REQUEST.FOREIGN_KEY_ID = UPRO_JOB_DESIGN.JOB_DESIGN_ID
GROUP BY 'Part ' + UPRO_PHASE_CODE.CODE_NUM + ' - ' + UPRO_PHASE_CODE.DESCR, UPRO_PHASE_CODE.SORT_ORDER, UPRO_PHASE_CODE.PHASE_CODE_ID, 
                         ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME
HAVING        (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt;= { fn NOW() })
ORDER BY PhaseSort</CommandText>
      </Query>
