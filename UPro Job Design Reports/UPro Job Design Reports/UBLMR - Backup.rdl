  <DataSets>
    <DataSet Name="Master">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@T">
            <Value>=Parameters!T.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PHASE">
            <Value>=Parameters!PHASE.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Complete">
            <Value>=Parameters!Complete.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        UPRO_JOB.JOB_ID, 'Part ' + UPRO_PHASE_CODE.CODE_NUM + ' - ' + UPRO_PHASE_CODE.DESCR AS Phase, UPRO_PHASE_CODE.SORT_ORDER AS PhaseSort, UPRO_SPECIFICATION.SPECIFICATION_NUM, 
                         UPRO_SPECIFICATION.DESCR AS SpecDescription, UPRO_SPECIFICATION.SORT_ORDER AS SpecSort, UPRO_JOB_LOCATION.LOCATION_NUM_CHAR AS Location, 
                         UPRO_JOB_LOCATION.SORT_ORDER AS LocationSort, UPRO_JOB_LOCATION_DETAIL.ESTIMATED_QTY, UPRO_JOB_LOCATION_DETAIL.COMPLETE, 
                         UPRO_JOB_LOCATION_DETAIL.COMPLETE_DATE AS SpecActionCompDate, UPRO_INVOICE.REFERENCE_NUM, UPRO_JOB.DESCR, UPRO_JOB.WORKGROUP_JOB_NUM, 
                         COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0) AS QTY, UPRO_JOB_LOCATION_DETAIL.COMMENTS, UPRO_JOB_LOCATION_DETAIL.JOB_LOCATION_DETAIL_ID, 
                         vw_RPT_Active_Invoice_Location_Details.INVOICE_LOCATION_DETAIL_ID, vw_RPT_Active_Invoice_Location_Details.QTY AS BillQTY, CASE WHEN UPRO_INVOICE.REFERENCE_NUM &lt;&gt; NULL AND 
                         COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0) &lt;&gt; COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) THEN COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) 
                         ELSE COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0) END AS CompletedQTY, CASE WHEN COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) &gt; 0 AND 
                         COALESCE (UPRO_JOB_LOCATION_DETAIL.ESTIMATED_QTY, 0) &lt;&gt; COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) THEN COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) 
                         ELSE COALESCE (UPRO_JOB_LOCATION_DETAIL.ESTIMATED_QTY, 0) END AS EstimatedQTY, UPRO_PHASE_CODE_SECTION.SORT_ORDER AS PhaseSectuionSort, 
                         UPRO_JOB_LOCATION_DETAIL.DESIGN_QTY, UPRO_PHASE_CODE.PHASE_CODE_ID, CASE WHEN dbo.UPRO_JOB_LOCATION_DETAIL.COMPLETE = 'N' THEN 'False' ELSE 'True' END AS Expr1, 
                         ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME
FROM            UPRO_PHASE_CODE INNER JOIN
                         UPRO_PHASE_CODE_SECTION ON UPRO_PHASE_CODE.PHASE_CODE_SECTION_ID = UPRO_PHASE_CODE_SECTION.PHASE_CODE_SECTION_ID RIGHT OUTER JOIN
                         ARPT_REPORT_REQUEST RIGHT OUTER JOIN
                         UPRO_JOB_DESIGN ON ARPT_REPORT_REQUEST.FOREIGN_KEY_ID = UPRO_JOB_DESIGN.JOB_DESIGN_ID LEFT OUTER JOIN
                         vw_RPT_Active_Invoice_Location_Details LEFT OUTER JOIN
                         UPRO_INVOICE_SUMMARY ON vw_RPT_Active_Invoice_Location_Details.INVOICE_SUMMARY_ID = UPRO_INVOICE_SUMMARY.INVOICE_SUMMARY_ID LEFT OUTER JOIN
                         UPRO_INVOICE ON UPRO_INVOICE_SUMMARY.INVOICE_ID = UPRO_INVOICE.INVOICE_ID RIGHT OUTER JOIN
                         UPRO_JOB_LOCATION_DETAIL RIGHT OUTER JOIN
                         UPRO_JOB_LOCATION ON UPRO_JOB_LOCATION_DETAIL.JOB_LOCATION_ID = UPRO_JOB_LOCATION.JOB_LOCATION_ID LEFT OUTER JOIN
                         UPRO_SPECIFICATION_ACTION ON UPRO_JOB_LOCATION_DETAIL.SPECIFICATION_ACTION_ID = UPRO_SPECIFICATION_ACTION.SPECIFICATION_ACTION_ID ON 
                         vw_RPT_Active_Invoice_Location_Details.LOCATION_DETAIL_ID = UPRO_JOB_LOCATION_DETAIL.JOB_LOCATION_DETAIL_ID ON 
                         UPRO_JOB_DESIGN.JOB_DESIGN_ID = UPRO_JOB_LOCATION.JOB_DESIGN_ID LEFT OUTER JOIN
                         UPRO_JOB ON UPRO_JOB_DESIGN.JOB_ID = UPRO_JOB.JOB_ID ON UPRO_PHASE_CODE_SECTION.ORG_ID = UPRO_JOB.OWNER_ID AND 
                         UPRO_PHASE_CODE.PHASE_CODE_ID = UPRO_SPECIFICATION_ACTION.PHASE_CODE_ID LEFT OUTER JOIN
                         UPRO_SPECIFICATION ON UPRO_PHASE_CODE_SECTION.PHASE_CODE_SECTION_ID = UPRO_SPECIFICATION.PHASE_CODE_SECTION_ID AND 
                         UPRO_SPECIFICATION_ACTION.SPECIFICATION_ID = UPRO_SPECIFICATION.SPECIFICATION_ID
GROUP BY UPRO_JOB.JOB_ID, 'Part ' + UPRO_PHASE_CODE.CODE_NUM + ' - ' + UPRO_PHASE_CODE.DESCR, UPRO_PHASE_CODE.SORT_ORDER, UPRO_SPECIFICATION.SPECIFICATION_NUM, 
                         UPRO_SPECIFICATION.DESCR, UPRO_SPECIFICATION.SORT_ORDER, UPRO_JOB_LOCATION.LOCATION_NUM_CHAR, UPRO_JOB_LOCATION.SORT_ORDER, UPRO_JOB_LOCATION_DETAIL.ESTIMATED_QTY, 
                         UPRO_JOB_LOCATION_DETAIL.COMPLETE, UPRO_JOB_LOCATION_DETAIL.COMPLETE_DATE, UPRO_INVOICE.REFERENCE_NUM, UPRO_JOB.DESCR, UPRO_JOB.WORKGROUP_JOB_NUM, 
                         COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0), UPRO_JOB_LOCATION_DETAIL.COMMENTS, UPRO_JOB_LOCATION_DETAIL.JOB_LOCATION_DETAIL_ID, 
                         vw_RPT_Active_Invoice_Location_Details.INVOICE_LOCATION_DETAIL_ID, vw_RPT_Active_Invoice_Location_Details.QTY, CASE WHEN UPRO_INVOICE.REFERENCE_NUM &lt;&gt; NULL AND 
                         COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0) &lt;&gt; COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) THEN COALESCE (vw_RPT_Active_Invoice_Location_Details.QTY, 0) 
                         ELSE COALESCE (UPRO_JOB_LOCATION_DETAIL.COMPLETED_TO_DATE, 0) END, UPRO_PHASE_CODE_SECTION.SORT_ORDER, UPRO_JOB_LOCATION_DETAIL.DESIGN_QTY, 
                         UPRO_PHASE_CODE.PHASE_CODE_ID, ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME
HAVING        (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt;= { fn NOW() }) AND 
                         (CASE WHEN dbo.UPRO_JOB_LOCATION_DETAIL.COMPLETE = 'N' THEN 'False' ELSE 'True' END = 'False') AND (UPRO_PHASE_CODE.PHASE_CODE_ID IN (@PHASE)) OR
                         (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt; { fn NOW() }) AND 
                         (CASE WHEN dbo.UPRO_JOB_LOCATION_DETAIL.COMPLETE = 'N' THEN 'False' ELSE 'True' END = @Complete) AND (UPRO_PHASE_CODE.PHASE_CODE_ID IN (@PHASE))
ORDER BY PhaseSectuionSort, PhaseSort, SpecSort, LocationSort</CommandText>
      </Query>
      <Fields>
