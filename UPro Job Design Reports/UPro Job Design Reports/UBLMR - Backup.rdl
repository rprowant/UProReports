    <DataSet Name="Header">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@T">
            <Value>=Parameters!T.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        vw_RPT_JobHeader.JOB_ID, vw_RPT_JobHeader.OwnerLogo, vw_RPT_JobHeader.OwnerLogo_mime, vw_RPT_JobHeader.WGLOGO, vw_RPT_JobHeader.WGLOGO_mime, 
                         ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME, ARPT_REPORT_REQUEST.FOREIGN_KEY_ID, 
                         UPRO_JOB_DESIGN.JOB_ID AS Expr1, vw_RPT_JobHeader.WORKGROUP_JOB_NUM, vw_RPT_JobHeader.JobDescription
FROM            UPRO_JOB_DESIGN LEFT OUTER JOIN
                         vw_RPT_JobHeader ON UPRO_JOB_DESIGN.JOB_ID = vw_RPT_JobHeader.JOB_ID RIGHT OUTER JOIN
                         ARPT_REPORT_REQUEST ON UPRO_JOB_DESIGN.JOB_DESIGN_ID = ARPT_REPORT_REQUEST.FOREIGN_KEY_ID
WHERE        (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt;= { fn NOW() })</CommandText>
      </Query>
      <Fields>
