  <DataSets>
    <DataSet Name="Master">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@T">
            <Value>=Parameters!T.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        UPRO_JOB_SCHEDULE.PLANNED_WEEK_CODE, UPRO_JOB_SCHEDULE.WEEK_ENDING_DATE, 
                         CASE WHEN PLANNED_WEEK_CODE = 0 THEN 'T' WHEN REVISED_PLANNED_DOLLARS &gt; 0 THEN 'T' WHEN ACTUAL_EARNED_DOLLARS &gt; 0 THEN 'T' ELSE 'F' END AS Filter, 
                         UPRO_JOB_SCHEDULE.REVISED_PLANNED_DOLLARS AS REVISED_PLANNED_HOURS, UPRO_JOB_SCHEDULE.ACTUAL_EARNED_DOLLARS AS ACTUAL_EARNED_HOURS, 
                         ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME, UPRO_JOB_SCHEDULE.SCHEDULE_TYPE_CODE, 
                         UPRO_JOB_SCHEDULE.JOB_DESIGN_ID
FROM            UPRO_JOB_SCHEDULE RIGHT OUTER JOIN
                         ARPT_REPORT_REQUEST ON UPRO_JOB_SCHEDULE.JOB_DESIGN_ID = ARPT_REPORT_REQUEST.FOREIGN_KEY_ID
WHERE        (CASE WHEN PLANNED_WEEK_CODE = 0 THEN 'T' WHEN REVISED_PLANNED_DOLLARS &gt; 0 THEN 'T' WHEN ACTUAL_EARNED_DOLLARS &gt; 0 THEN 'T' ELSE 'F' END = 'T')
GROUP BY UPRO_JOB_SCHEDULE.PLANNED_WEEK_CODE, UPRO_JOB_SCHEDULE.WEEK_ENDING_DATE, ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, 
                         ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME, UPRO_JOB_SCHEDULE.REVISED_PLANNED_DOLLARS, UPRO_JOB_SCHEDULE.ACTUAL_EARNED_DOLLARS, 
                         UPRO_JOB_SCHEDULE.SCHEDULE_TYPE_CODE, UPRO_JOB_SCHEDULE.JOB_DESIGN_ID
HAVING        (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt;= { fn NOW() }) AND 
                         (UPRO_JOB_SCHEDULE.SCHEDULE_TYPE_CODE = 'WEEK')
ORDER BY UPRO_JOB_SCHEDULE.WEEK_ENDING_DATE</CommandText>
      </Query>
