    <DataSet Name="WeekEnding">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@T">
            <Value>=Parameters!T.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        vw_Job_Workdown_Week_End.WeekEndDate, ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, DATEADD(d, - 7, vw_Job_Workdown_Week_End.DefaultDate) 
                         AS DefaultDate, vw_Job_Workdown_Week_End.JOB_ID, YEAR(vw_Job_Workdown_Week_End.WeekEndDate) AS Year, { fn WEEK(vw_Job_Workdown_Week_End.WeekEndDate) } AS Week
FROM            vw_Job_Workdown_Week_End RIGHT OUTER JOIN
                         UPRO_JOB_DESIGN ON vw_Job_Workdown_Week_End.JOB_ID = UPRO_JOB_DESIGN.JOB_ID RIGHT OUTER JOIN
                         ARPT_REPORT_REQUEST ON UPRO_JOB_DESIGN.JOB_DESIGN_ID = ARPT_REPORT_REQUEST.FOREIGN_KEY_ID
GROUP BY vw_Job_Workdown_Week_End.WeekEndDate, DATEADD(d, - 7, vw_Job_Workdown_Week_End.DefaultDate), ARPT_REPORT_REQUEST.SEC_TOKEN, ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, 
                         vw_Job_Workdown_Week_End.JOB_ID, YEAR(vw_Job_Workdown_Week_End.WeekEndDate), { fn WEEK(vw_Job_Workdown_Week_End.WeekEndDate) }
HAVING        (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (vw_Job_Workdown_Week_End.WeekEndDate &lt; DATEADD(d, 7, { fn NOW() }))
ORDER BY Year DESC, Week DESC, vw_Job_Workdown_Week_End.WeekEndDate DESC</CommandText>
      </Query>
      <Fields>
        <Field Name="WeekEndDate">
          <DataField>WeekEndDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SEC_TOKEN">
          <DataField>SEC_TOKEN</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ARPT_REPORT_REQUEST_ID">
          <DataField>ARPT_REPORT_REQUEST_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="DefaultDate">
          <DataField>DefaultDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="JOB_ID">
          <DataField>JOB_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Week">
          <DataField>Week</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
