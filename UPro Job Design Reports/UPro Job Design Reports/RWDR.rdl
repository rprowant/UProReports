                         vw_RPT_JobHeader.WORKGROUP_JOB_NUM, vw_RPT_JobHeader.JobDescription, ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID, ARPT_REPORT_REQUEST.SEC_TOKEN, 
                         ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME, vw_RPT_JobHeader.JOB_ID
FROM            UPRO_JOB_DESIGN LEFT OUTER JOIN
                         vw_RPT_JobHeader ON UPRO_JOB_DESIGN.JOB_ID = vw_RPT_JobHeader.JOB_ID RIGHT OUTER JOIN
                         ARPT_REPORT_REQUEST ON UPRO_JOB_DESIGN.JOB_DESIGN_ID = ARPT_REPORT_REQUEST.FOREIGN_KEY_ID
WHERE        (ARPT_REPORT_REQUEST.ARPT_REPORT_REQUEST_ID = @RID) AND (ARPT_REPORT_REQUEST.SEC_TOKEN = @T) AND (ARPT_REPORT_REQUEST.EXPIRATION_DATE_TIME &gt;= { fn NOW() })</CommandText>
