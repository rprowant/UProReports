  <DataSets>
    <DataSet Name="Master">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Dept">
            <Value>=Parameters!Dept.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        UPRO_PERSON.PERSON_ID, UPRO_PERSON.FULL_NAME, COALESCE (vw_RPT_EmployeeCrewAssignment.crewname, 'Not Assigned') AS CrewName, 
                         COALESCE (vw_RPT_EmployeeCrewAssignment.department, 'Not Assigned') AS Department, UPRO_CRAFT.CRAFT
FROM            UPRO_CRAFT RIGHT OUTER JOIN
                         UPRO_PERSON ON UPRO_CRAFT.CRAFT_ID = UPRO_PERSON.CRAFT_ID LEFT OUTER JOIN
                         vw_RPT_EmployeeCrewAssignment ON UPRO_PERSON.PERSON_ID = vw_RPT_EmployeeCrewAssignment.person_id
WHERE        (UPRO_PERSON.ACTIVE = 'Y') AND (UPRO_PERSON.ORG_ID = 37) OR
                         (UPRO_PERSON.ACTIVE = 'Y') AND (UPRO_PERSON.ORG_ID = 37)
GROUP BY UPRO_PERSON.PERSON_ID, UPRO_PERSON.FULL_NAME, COALESCE (vw_RPT_EmployeeCrewAssignment.crewname, 'Not Assigned'), COALESCE (vw_RPT_EmployeeCrewAssignment.department, 
                         'Not Assigned'), UPRO_CRAFT.CRAFT
HAVING        (COALESCE (vw_RPT_EmployeeCrewAssignment.department, 'Not Assigned') IN (@Dept)) OR
                         (COALESCE (vw_RPT_EmployeeCrewAssignment.department, 'Not Assigned') = 'Not Assigned')
ORDER BY UPRO_PERSON.FULL_NAME</CommandText>
      </Query>
      <Fields>
        <Field Name="PERSON_ID">
          <DataField>PERSON_ID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="FULL_NAME">
          <DataField>FULL_NAME</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CrewName">
          <DataField>CrewName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Department">
          <DataField>Department</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CRAFT">
          <DataField>CRAFT</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Depart">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <CommandText>SELECT        MEANING
FROM            UPRO_SMART_CODES
WHERE        (SMART_TYPE = 'Department')
GROUP BY MEANING
ORDER BY MEANING</CommandText>
      </Query>
      <Fields>
        <Field Name="MEANING">
          <DataField>MEANING</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
