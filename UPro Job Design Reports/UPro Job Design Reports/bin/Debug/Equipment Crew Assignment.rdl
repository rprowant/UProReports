  <DataSets>
    <DataSet Name="Master">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Depart">
            <Value>=Parameters!Depart.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        UPRO_EQUIPMENT.EQUIPMENT_NUM, UPRO_EQUIPMENT.DESCR, COALESCE (vw_RPT_CrewEquipment.CrewName, 'Not Assigned') AS CrewName, COALESCE (vw_RPT_CrewEquipment.Department, 
                         'Not Assigned') AS Department, UPRO_EQUIPMENT.EQUIPMENT_ID, UPRO_EQUIPMENT_CATEGORY.CATEGORY, UPRO_EQUIPMENT.ACTIVE_FLAG
FROM            UPRO_EQUIPMENT_CATEGORY RIGHT OUTER JOIN
                         UPRO_EQUIPMENT ON UPRO_EQUIPMENT_CATEGORY.EQUIPMENT_CATEGORY_ID = UPRO_EQUIPMENT.CATEGORY_ID LEFT OUTER JOIN
                         vw_RPT_CrewEquipment ON UPRO_EQUIPMENT.EQUIPMENT_ID = vw_RPT_CrewEquipment.EQUIPMENT_ID
WHERE        (UPRO_EQUIPMENT.ACTIVE_FLAG = 'Y') OR
                         (UPRO_EQUIPMENT.ACTIVE_FLAG = 'Y')
GROUP BY UPRO_EQUIPMENT.EQUIPMENT_NUM, UPRO_EQUIPMENT.DESCR, COALESCE (vw_RPT_CrewEquipment.CrewName, 'Not Assigned'), COALESCE (vw_RPT_CrewEquipment.Department, 'Not Assigned'), 
                         UPRO_EQUIPMENT.EQUIPMENT_ID, UPRO_EQUIPMENT_CATEGORY.CATEGORY, UPRO_EQUIPMENT.ACTIVE_FLAG
HAVING        (COALESCE (vw_RPT_CrewEquipment.Department, 'Not Assigned') IN (@Depart)) OR
                         (COALESCE (vw_RPT_CrewEquipment.Department, 'Not Assigned') = 'Not Assigned')
ORDER BY UPRO_EQUIPMENT.EQUIPMENT_NUM</CommandText>
      </Query>
      <Fields>
        <Field Name="EQUIPMENT_NUM">
