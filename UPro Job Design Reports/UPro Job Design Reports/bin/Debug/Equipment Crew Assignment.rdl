    <DataSet Name="Depart">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <CommandText>SELECT        MEANING
FROM            UPRO_SMART_CODES
WHERE        (SMART_TYPE = 'Department')
GROUP BY MEANING
ORDER BY MEANING</CommandText>
      </Query>
      <Fields>
        <Field Name="MEANING">
          <DataField>MEANING</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
